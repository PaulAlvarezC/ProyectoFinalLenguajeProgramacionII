{
  "expo": {
    "name": "Dear",
    "slug": "Dear",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoDear.png",
    "splash": {
      "image": "./assets/splashDear.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.itq.dear",
      "supportsTablet": true
    },
    "android": {
      "package": "com.itq.dear",
      "config": {
      },
      "permissions": [
        "CAMERA_ROLL",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "App Dear"
  }
}
